{
  "rewrites": [
    { "source": "/dashboard", "destination": "https://app.thelabelai.com" },
    { "source": "/dashboard/(.*)", "destination": "https://app.thelabelai.com/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
